#include <iostream>
#include <string>

int main() {
    // Write C++ code here
    std::string full_name;
    int age;
    
    std::cout << "Enter your full name and age" << std::endl;
    std::getline(std::cin,full_name);
    std::cin >> age;
    
    std::cout << "Hello " << full_name << ", you are " << 
            age << " years old";

    //default for, "this program has no issues"
    return 0;
}
